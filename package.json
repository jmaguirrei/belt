{
  "name": "@jmaguirrei/belt",
  "version": "0.0.12",
  "description": "Minimal Lodash like utilities",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmaguirrei/belt.git"
  },
  "scripts": {
    "build": "parcel build ./src/index.js",
    "git_": "git add . && git commit -m 'Initial commit' && git push",
    "pack_": "npm version patch && npm run build && npm publish --access public",
    "git-pack": "npm run git_ && npm run pack_",
    "update-jmaguirrei": "ncu -u '/^@jmaguirrei.*$/' && npm install",
    "test": "nodemon tests.js"
  },
  "author": "Jos√© Manuel Aguirre",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0"
  }
}
